{
    "compilerOptions": {
      "sourceMap": true,
      "noImplicitAny": true,
      "module": "commonjs",
      "target": "es6",
      "allowJs": true,
      "esModuleInterop": true,
      "moduleResolution": "node",
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true
    },
    "include": [
      "**/*.ts",
      ".sequelizerc"
    ],
    "exclude": [
        "node_modules"
    ],
  }